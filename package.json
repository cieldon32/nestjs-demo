{
  "name": "test-fullstack",
  "version": "0.0.1",
  "description": "",
  "author": "cieldon",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start -w src",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^6.0.0",
    "@nestjs/config": "^0.1.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/graphql": "^6.6.2",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/passport": "^6.1.1",
    "@nestjs/platform-express": "^6.10.14",
    "@nestjs/platform-socket.io": "^9.2.0",
    "@nestjs/platform-ws": "^6.11.1",
    "@nestjs/websockets": "^6.11.1",
    "@typegoose/typegoose": "^6.2.1",
    "@zeit/next-sass": "^1.0.1",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.15",
    "mongoose": "^5.8.9",
    "nest-next-module": "^0.5.4",
    "nestjs-mongoose-crud": "^1.2.2",
    "nestjs-typegoose": "^7.0.0",
    "next": "^9.2.0",
    "node-sass": "^4.13.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-saml": "^1.2.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.5.4",
    "swagger-ui-express": "^4.1.2",
    "typegoose-cursor-pagination": "^1.1.0"
  },
  "devDependencies": {
    "@nestjs/swagger": "^4.1.14",
    "@nestjs/testing": "^6.0.0",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.0",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "4.16.1",
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.149",
    "@types/mongoose": "^5.5.41",
    "@types/node": "^13.1.8",
    "@types/passport": "^1.0.2",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/react": "^16.9.17",
    "@types/socket.io": "^2.1.4",
    "@types/supertest": "2.0.7",
    "dotenv": "^8.2.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "jest": "^24.7.1",
    "prettier": "1.17.0",
    "react-test-renderer": "^16.12.0",
    "supertest": "4.0.2",
    "ts-jest": "^24.0.2",
    "ts-loader": "^7.0.4",
    "ts-node": "8.1.0",
    "tsc-watch": "2.2.1",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "typescript": "^3.7.5",
    "typings-for-css-modules-loader": "^1.7.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "@libs/db/(.*)": "<rootDir>/libs/db/src/$1",
      "@libs/crud/(.*)": "<rootDir>/libs/crud/src/$1",
      "@libs/common/(.*)": "<rootDir>/libs/common/src/$1"
    }
  }
}
